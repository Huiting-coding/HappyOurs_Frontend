}

var shifter = function (i) {
  if (i == "whiskey") {
    return whiskey;
  } else if (i == "gin") {
    return gin;
  } else if (i == "mixed") {
    return mixed;
  } else if (i == "beer") {
    return beer;
  } else if (i == "martini") {
    return martini;
  } else if (i == "vodka") {
    return vodka;
  } else if (i == "wine") {
    return wine;
  } else if (i == "other") {
    return other;
  } else if (i == "coffee") {
    return coffee;
  } else if (i == "tea") {
    return tea;
  } else {
    return cauldron;
  }
};


module.exports = {
  beer: beer,
  martini: martini,
  whiskey: whiskey,
